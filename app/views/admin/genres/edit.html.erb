<div class='container'>
  <div class='row'>
    <div class='col mt-4'>
      <h3>ジャンル編集</h3>
      <%= form_with model: @genre, url: admin_genre_path(@genre), method: :patch do |f| %>
      <div class="mt-4">
        <%= f.label "ジャンル名" %>
        <%= f.text_field :name %>
        <%= f.submit "変更を保存", class:"btn btn-success" %>
      </div>
      <% end %>
    </div>
  </div>
</div>